#include <stdio.h>

int main() {
    int vetor[5];

    // LÃª os 5 valores do vetor
    printf("Digite 5 valores:\n");
    for (int i = 0; i < 5; i++) {
        printf("Valor %d: ", i + 1);
        scanf("%d", &vetor[i]);
    }

    // Imprime o vetor invertido
    printf("\nVetor invertido:\n");
    for (int i = 4; i >= 0; i--) {
        printf("%d ", vetor[i]);
    }
    printf("\n");

    return 0;
}